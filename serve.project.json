{
	"name": "flamework-game",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$path": "out/server"
		},
		"ReplicatedFirst": {
			"$className": "ReplicatedFirst",
			"$path": "out/preload"
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"stories": {
				"$path": "out/stories"
			},
			"$path": "out/shared"
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterCharacter": {
				"$path": "out/StarterCharacter.rbxm"
			},
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"$path": "out/client"
			},
			"StarterCharacterScripts": {
				"$className": "StarterCharacterScripts",
				"$path": "out/character"
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true,
				"Gravity": 35
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$path": "out/sounds",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Lighting": {
			"$path": "out/lighting.rbxm",
			"$properties": {
				"Brightness": 0,
				"ShadowSoftness": 1,
				"ClockTime": 14.5,
				"GeographicLatitude": 0,
				"GlobalShadows": true,
				"Ambient": [0.337255, 0.356863, 0.294118],
				"OutdoorAmbient": [0, 0, 0],
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1,
				"Technology": "Future"
			}
		}
	}
}
